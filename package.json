{
  "name": "@soulofmischief/math-utils",
  "description": "A lil Javascript math library",
  "version": "1.1.3",
  "author": {
    "name": "soulofmischief",
    "email": "soulofmischief@protonmail.com",
    "url": "https://github.com/soulofmischief"
  },
  "homepage": "https://github.com/soulofmischief/math-utils",
  "license": "MIT",
  "bugs": {},
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "repository": {},
  "scripts": {
    "build": "npm run clean; for script in {commonjs,es,umd,umd:min}; do npm run build:$script; done",
    "build:commonjs": "BABEL_ENV=commonjs npx babel src --out-dir lib",
    "build:es": "BABEL_ENV=es npx babel src --out-dir es",
    "build:umd": "BABEL_ENV=es NODE_ENV=development npx rollup src/index.js --config --sourcemap --file dist/math-utils.js",
    "build:umd:min": "BABEL_ENV=es NODE_ENV=production npx rollup src/index.js --config --file dist/math-utils.min.js",
    "clean": "npx rimraf es dist lib",
    "lint:flow": "npx flow status",
    "lint:flow:stop": "npx flow stop",
    "prepublishOnly": "npm run build",
    "test": "babel-node node_modules/.bin/ospec"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-flow": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@soulofmischief/flow-types": "^0.0.1",
    "babel-plugin-macros": "^3.0.1",
    "flow-bin": "^0.143.1",
    "ospec": "^4.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.0",
    "rollup-plugin-uglify": "^6.0.4"
  }
}
